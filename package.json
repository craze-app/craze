{
  "name": "craze",
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate.",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "tsc && vite build && electron-builder",
    "build:vite": "tsc && vite build",
    "build:macos": "tsc && vite build && electron-builder -m",
    "build:win": "tsc && vite build && electron-builder -w",
    "build:linux": "tsc && vite build && electron-builder -l",
    "postinstall": "patch-package",
    "generate-feature": "node scripts/generate-feature.js"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@jest/globals": "^29.3.1",
    "@tabler/icons": "^1.118.0",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.0",
    "ace-builds": "^1.14.0",
    "classnames": "^2.3.2",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "formik": "^2.2.9",
    "fuse.js": "^6.6.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lodash": "^4.17.21",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.1",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-modern-drawer": "^1.1.2",
    "react-reflex": "^4.0.9",
    "react-router-dom": "5.3.4",
    "sass": "^1.57.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-electron-plugin": "^0.6.4",
    "vite-plugin-electron-renderer": "^0.11.3",
    "zustand": "^4.2.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {}
}
